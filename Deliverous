blog:
  image: deliverous/blog
  ports:
  - ip: blog
    container_port: 80
    host_port: 80

mysql:
    image: mysql
    environment:
        MYSQL_ROOT_PASSWORD: admin

wordpress
    image: wordpress
    environment:
        WORDPRESS_DB_PASSWORD: admin
    ports:
    - ip: wordpress
      container_port: 80
      host_port: 80

